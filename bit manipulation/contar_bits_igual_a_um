#Contar quantos bits 1 existem em um número
.data
	numero: .space 100
.text
	lw $t0, numero
	li $v0, 5
	syscall
	move $t0, $v0
	li $t1, 0 #total de 1's
	li $t2, 0 #iteração
	
	loop:
		beq $t2, 32, printar
		andi $t3, $t0, 1
		beq $t3, 1, adicionar_1
		srl $t0, $t0, 1
		addi $t2, $t2, 1
		j loop
	
	adicionar_1:
		addi $t1, $t1, 1
		addi $t2, $t2, 1
		srl $t0, $t0, 1
		j loop
	
	printar:
		li $v0, 1
		move $a0, $t1
		syscall
		li $v0, 10
		syscall